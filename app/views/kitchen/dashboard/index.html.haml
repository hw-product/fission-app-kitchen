%div.kitchen-dashboard
  %div.container.container-padding
    %div.panel.panel-default
      %div.panel-heading
        Kitchen Dashboard
      %div.panel-body#kitchen-dashboard
        - @data.each do |acct, repos|
          %table.table.table-hover
            %thead
              %tr
                %th= acct.name
                %th Completed successfully
                %th Log data
            - repos.each do |repo, last_job|
              %tr
                %td= repo.name
                %td= last_job[:completed]
                %td= last_job[:logs]
